{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ha-ipaper debug",
            "type": "python",
            "request": "launch",
            "program": "ha-ipaper/__main__.py",
            "env":
            {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "-config", "debug.yaml"
            ],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}